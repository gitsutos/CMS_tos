generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cost {
  id          String  @id @default(cuid())
  description String?
  amount      Decimal @default(0) @db.Decimal
  user        String  @db.Uuid

  @@map("cost")
}

